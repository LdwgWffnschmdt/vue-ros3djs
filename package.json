{
  "name": "vue-ros3djs",
  "version": "1.0.4",
  "description": "Vue components for ros3djs",
  "keywords": [
    "vue",
    "vuejs",
    "ros",
    "rviz",
    "ros3d",
    "ros3djs"
  ],
  "author": "Ludwig Waffenschmidt <l.waffenschmidt@wzl.rwth-aachen.de>",
  "main": "dist/vue-ros3djs.ssr.js",
  "module": "dist/vue-ros3djs.esm.js",
  "unpkg": "dist/vue-ros3djs.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/lib-dev.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.3.2",
    "ros3d": "^1.0.1",
    "three": "^0.89.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "minimist": "^1.2.0",
    "rollup": "^1.26.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "5.1.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
